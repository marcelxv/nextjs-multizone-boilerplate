{
  "name": "nextjs-multizone-boilerplate",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "test": "turbo run test",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "e2e": "turbo run e2e",
    "e2e:ui": "turbo run e2e:ui",
    "e2e:web": "turbo run e2e --filter=@workspace/e2e-web --only",
    "e2e:admin": "turbo run e2e --filter=@workspace/e2e-admin --only"
  },
  "devDependencies": {
    "@eslint/js": "^9.26.0",
    "@playwright/test": "1.52.0",
    "@workspace/eslint-config": "workspace:*",
    "@workspace/typescript-config": "workspace:*",
    "playwright": "1.52.0",
    "prettier": "^3.5.1",
    "turbo": "^2.4.2",
    "typescript": "5.7.3"
  },
  "packageManager": "pnpm@10.10.0",
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "@types/node": "^22.15.17"
  }
}
